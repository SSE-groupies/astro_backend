# Example environment file - Copy to .env for local development

# Azure Storage settings
AZURE_STORAGE_ACCOUNT_URL=https://yourstorageaccount.table.core.windows.net
AZURE_STORAGE_CONNECTION_STRING=DefaultEndpointsProtocol=https;AccountName=youraccount;AccountKey=yourkey;EndpointSuffix=core.windows.net
USE_MANAGED_IDENTITY=false

# Redis settings
REDIS_HOST=yourredishost.redis.cache.windows.net
REDIS_SSL=true
REDIS_PORT=6380
REDIS_PASSWORD=yourredispassword

# Application settings
ENVIRONMENT=development
PORT=8080
AZURE_MONITORING=false
